"""
The two methods are listed below
"""
x=0
i=0
y=0
j=0
z=0
k=0
while(3*i<1000):
    x+=3*i
    i+=1
while(5*j<1000):
    y+=5*j
    j+=1
while(15*k<1000):
    z+=15*k
    k+=1
print(x+y-z)

i=0
sum=0
for i in range(1000):
    if(i%3==0 or i%5==0):
        sum+=i
        i+=1
print(sum)
